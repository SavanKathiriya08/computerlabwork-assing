#include <stdio.h>

int main() {
float fahrenheit, celsius;

// Input from user
printf("Enter temperature in Fahrenheit: ");
scanf("%f", &fahrenheit);

// Conversion formula
celsius = (fahrenheit - 32) * 5 / 9;

// Output result
printf("%.2f Fahrenheit is %.2f Celsius\n", fahrenheit, celsius);

return 0;
}
